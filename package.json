                {
                    "id": "itemlog",
                    "name": "File History",
                    "when": "config.svn.enabled && svnOpenRepositoryCount != 0"
                },
                {
                    "id": "branchchanges",
                    "name": "Branch Changes",
                    "when": "config.svn.enabled && svnOpenRepositoryCount != 0"
                }
            ]
        },
