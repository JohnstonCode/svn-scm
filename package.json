{
    "name": "svn-scm",
    "displayName": "SVN",
    "description": "Integrated Subversion source control",
    "version": "2.10.3",
    "publisher": "johnstoncode",
    "engines": {
        "vscode": "^1.42.0"
    },
    "private": true,
    "enableProposedApi": false,
    "icon": "images/subversion.png",
    "homepage": "https://github.com/JohnstonCode/svn-scm/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/JohnstonCode/svn-scm.git"
    },
    "bugs": {
        "url": "https://github.com/JohnstonCode/svn-scm/issues"
    },
    "categories": [
        "Other",
        "SCM Providers"
    ],
    "keywords": [
        "multi-root ready",
        "scm",
        "svn",
        "subversion"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "scripts": {
        "build": "yarn run build:ts && yarn run build:css",
        "build:css": "yarn node-sass scss/ -o css/ --output-style compressed",
        "build:ts": "yarn tsc -p ./",
        "compile": "yarn tsc -watch -p ./",
        "lint": "eslint \"src/**/*.ts\"",
        "lint:fix": "yarn run lint --fix",
        "organize": "node ./out/tools/organize.js",
        "semantic-release": "semantic-release --yarn",
        "style-check": "yarn prettylint 'src/**/*.ts'",
        "style-fix": "yarn prettier --write \"src/**/*.ts\"",
        "test": "node ./out/test/runTest.js",
        "tools:genReadme": "node ./out/tools/generateConfigSectionForReadme.js",
        "vscode:prepublish": "yarn run lint && yarn run build",
        "watch:css": "yarn run build:css -w"
    },
    "dependencies": {
        "chardet": "^0.8.0",
        "dayjs": "^1.8.21",
        "minimatch": "^3.0.4",
        "original-fs": "^1.0.0",
        "semver": "^7.0.0",
        "tmp": "0.1.0",
        "xml2js": "^0.4.19"
    },
    "devDependencies": {
        "@semantic-release/changelog": "^5.0.0",
        "@semantic-release/git": "^9.0.0",
        "@types/chardet": "^0.8.0",
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.0",
        "@types/node": "^12.11.7",
        "@types/semver": "^7.1.0",
        "@types/tmp": "0.1.0",
        "@types/vscode": "1.42.0",
        "@types/xml2js": "^0.4.4",
        "@typescript-eslint/eslint-plugin": "^2.21.0",
        "@typescript-eslint/parser": "^2.21.0",
        "decache": "^4.5.1",
        "eslint": "^6.5.1",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-prettier": "^3.1.2",
        "glob": "^7.1.4",
        "milligram": "^1.3.0",
        "mocha": "^7.1.0",
        "node-sass": "^4.13.1",
        "prettier": "^1.18.2",
        "prettylint": "^1.0.0",
        "semantic-release": "^17.0.4",
        "semantic-release-vsce": "^3.0.1",
        "typescript": "^3.8.2",
        "vscode-test": "^1.0.0"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "svnActivity",
                    "title": "Subversion",
                    "icon": "icons/subversion-logo.svg"
                }
            ]
