                },
                {
                    "command": "svn.patchChangeList",
                    "when": "config.svn.enabled && scmProvider == svn"
                }
            ],
            "scm/sourceControl": [
                {
                    "command": "svn.close",
                    "group": "navigation",
                    "when": "scmProvider == svn"
                }
            ],
